{
  "name": "carparker-server",
  "version": "1.0.0",
  "description": "Server for CarParker app",
  "license": "UNLICENSED",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "debug": "env DEBUG=* node server.js",
    "pretest": "eslint .",
    "test": "NODE_ENV=test istanbul cover _mocha -- -b --recursive --require co-mocha",
    "test-ci": "NODE_ENV=test istanbul cover _mocha -- -b --recursive --require co-mocha -R spec && codecov"
  },
  "engines": {
    "node": "5.6.0",
    "npm": "3.6.0"
  },
  "homepage": "https://bitbucket.org/carparkerapp/carparker-server",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:carparkerapp/carparker-server.git"
  },
  "dependencies": {
    "bluebird": "3.3.3",
    "body-parser": "1.15.0",
    "bunyan": "1.7.1",
    "co": "4.6.0",
    "co-foreach": "1.1.1",
    "config": "1.19.0",
    "cron": "1.1.0",
    "express": "4.13.4",
    "express-bunyan-logger": "1.2.0",
    "faker": "3.0.1",
    "http-status-codes": "1.0.6",
    "joi": "8.0.4",
    "lodash": "4.6.1",
    "moment": "2.11.2",
    "mongoose": "4.4.6",
    "mongoose-hidden": "0.6.1",
    "newrelic": "1.25.4",
    "rollbar": "0.5.14",
    "superagent": "1.7.2"
  },
  "devDependencies": {
    "bunyan-prettystream": "0.1.3",
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "co-mocha": "1.1.2",
    "codecov": "^1.0.1",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-config-cp": "1.1.0",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "nock": "7.2.2",
    "sinon": "1.17.3",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.0.0"
  }
}
